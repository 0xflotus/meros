{
	"name": "meros",
	"version": "0.0.3",
	"description": "A fast utility for reading streamed multipart responses.",
	"keywords": [
		"multipart",
		"multipart mixed",
		"fetch",
		"utility",
		"reader"
	],
	"repository": "maraisr/meros",
	"license": "MIT",
	"author": {
		"name": "Marais Rossouw",
		"email": "me@marais.dev",
		"url": "https://marais.io"
	},
	"exports": {
		".": {
			"import": "./dist/node.mjs",
			"browser": "./dist/browser.mjs",
			"require": "./dist/node.js"
		}
	},
	"main": "dist/node.js",
	"unpkg": "dist/browser.min.js",
	"module": "dist/node.mjs",
	"types": "types/index.d.ts",
	"files": [
		"dist",
		"types"
	],
	"scripts": {
		"bench": "node -r ts-node/register bench/index.ts",
		"build": "rollup -c",
		"format": "prettier \"{*,tests/*,src/**/*,bench/*,.github/**/*}.+(yml|md)\" --write",
		"prepublishOnly": "yarn build",
		"test": "uvu -r ts-node/register -i server tests"
	},
	"devDependencies": {
		"@marais/tsconfig": "^0.0.1",
		"@rollup/plugin-node-resolve": "10.0.0",
		"@types/benchmark": "^2.1.0",
		"@types/node": "^14.14.6",
		"@types/node-fetch": "^2.5.7",
		"benchmark": "^2.1.4",
		"fetch-multipart-graphql": "^2.1.0",
		"node-fetch": "^2.6.1",
		"playwright": "^1.5.2",
		"polka": "^0.5.2",
		"prettier": "^2.1.2",
		"rollup": "2.33.1",
		"rollup-plugin-terser": "7.0.2",
		"rollup-plugin-typescript2": "0.29.0",
		"ts-node": "^9.0.0",
		"typescript": "^4.0.5",
		"uvu": "^0.4.0"
	},
	"volta": {
		"node": "15.1.0",
		"yarn": "1.22.5"
	}
}
