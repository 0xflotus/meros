{
	"name": "fmg",
	"version": "0.1.0",
	"scripts": {
		"format": "prettier \"{*,tests/*,src/*,.github/**/*}.+(ts|yml)\" --write",
		"build": "tsc --noEmit false --declaration",
		"test": "uvu tests -i util -i prelude -r ts-node/register",
		"bench": "node -r ts-node/register bench/index.ts",
		"test:coverage": "c8 yarn run test"
	},
	"prettier": {
		"bracketSpacing": true,
		"printWidth": 80,
		"proseWrap": "always",
		"singleQuote": true,
		"tabWidth": 4,
		"trailingComma": "all",
		"useTabs": true
	},
	"devDependencies": {
		"@marais/tsconfig": "^0.0.1",
		"@types/benchmark": "^1.0.33",
		"@types/node": "^14.0.14",
		"benchmark": "^2.1.4",
		"c8": "^7.2.0",
		"fetch-multipart-graphql": "^2.0.1",
		"node-fetch": "^2.6.0",
		"prettier": "^2.0.5",
		"rxjs": "^7.0.0-beta.1",
		"ts-node": "^8.10.2",
		"typescript": "^3.9.6",
		"uvu": "^0.0.17",
		"web-streams-polyfill": "^2.1.1"
	},
	"volta": {
		"node": "14.5.0",
		"yarn": "1.22.4"
	}
}
